# Основной домен платформы (обязательное поле)
SERVER_DOMAIN=your-domain.com

# Публичный IP сервера (обязательное поле)
SERVER_IP=your-public-ip

# Доступ к ARI (Asterisk REST Interface)
ARI_USER=ari
ARI_PASSWORD=your-ari-password

# Redis (временные токены вызовов)
REDIS_PASSWORD=your-redis-password

# TURN (Coturn)
TURN_USER=turn
TURN_PASSWORD=your-turn-password

# Postgres (хранение push-токенов)
POSTGRES_DB=intercom
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your-postgres-password

# Интеграция с домофоном
REALPHONE=1

# Expo Push
EXPO_ACCESS_TOKEN=your-expo-access-token
